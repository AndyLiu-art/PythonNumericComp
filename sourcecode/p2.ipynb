   "source": [
    "# 定义函数\n",
    "def integrand(x, a, b):\n",
    "    return a*x**2 + b\n",
    "\n",
    "a = 2\n",
    "b = 1"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "id": "bfffbe49-d22a-42d6-8004-7b62550ae475",
   "metadata": {
